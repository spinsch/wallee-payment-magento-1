<config>
	<menu>
	</menu>
	<acl>
		<resources>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<wallee_payment translate="title">
										<title>Wallee Payment</title>
										<sort_order>100</sort_order>
									</wallee_payment>
								</children>
							</config>
						</children>
					</system>
					<customer>
						<children>
							<manage>
								<children>
									<wallee_token translate="title">
		                                <title>Manage Wallee Tokens</title>
		                                <sort_order>100</sort_order>
		                            </wallee_token>
								</children>
							</manage>
						</children>
					</customer>
				</children>
			</admin>
		</resources>
	</acl>
</config>